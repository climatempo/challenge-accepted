{
  "name": "climatempo-api",
  "version": "0.0.1",
  "description": "ClimaTempo API",
  "scripts": {
    "dev": "ts-node -r dotenv/config -P tsconfig.json src/server.ts",
    "start": "gulp && BUILD=true node -r dotenv/config build/src/server.js",
    "migration:create": "typeorm migration:create --dir src/migrations --name",
    "migration:generate": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:generate --name",
    "migration:revert": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:run",
    "eslint:fix": "cross-env eslint --fix '**/*.{t,j}s'",
    "eslint:check": "cross-env eslint '**/*.{t,j}s'",
    "prettier:fix": "cross-env prettier --write '{src,tests}/**/*.{t,j}s'",
    "prettier:check": "cross-env prettier --check '{src,tests}/**/*.{t,j}s'",
    "postgres:start": "docker run -d -p 5430:5432 --name postgres -e POSTGRES_HOST_AUTH_METHOD=trust postgres:11-alpine",
    "database:create": "docker exec postgres psql -U postgres postgres -c \"CREATE DATABASE $(grep DB_DATABASE .env | cut -d= -f 2)\"",
    "typecheck": "npx tsc -noEmit",
    "test": "jest --setupFiles dotenv/config -w9 --detectOpenHandles"
  },
  "dependencies": {
    "@sentry/node": "^5.15.5",
    "@types/jsonwebtoken": "^8.5.0",
    "ajv": "^6.12.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "typeorm": "^0.2.24",
    "typeorm-naming-strategies": "^1.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/body-parser": "^1.19.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.11",
    "@types/http-status": "^1.1.2",
    "@types/jest": "^25.2.1",
    "@types/morgan": "^1.9.0",
    "@types/node": "^12.12.37",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "@typescript-eslint/typescript-estree": "^2.29.0",
    "axios-mock-adapter": "^1.18.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "faker": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^25.5.2",
    "lint-staged": "^10.2.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
